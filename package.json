{
  "name": "firstreact",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kamil-kamil2.github.io/React-Movies",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.x.x",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "dev": "cd frontend && vite",
    "build": "cd frontend && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "cd frontend && vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d frontend/dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
